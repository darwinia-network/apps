{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=2592000"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/account", "destination": "/index.html" },
    { "source": "/staking", "destination": "/index.html" },
    { "source": "/toolbox", "destination": "/index.html" },
    { "source": "/portal", "destination": "/index.html" },
    { "source": "/migration", "destination": "/index.html" }
  ]
}
