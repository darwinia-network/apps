{
  "name": "apps-light",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env CHOKIDAR_USEPOLLING=1 PORT=3006 craco start",
    "start:dev": "cross-env PORT=3006 REACT_APP_HOST_TYPE=dev craco start",
    "build": "cross-env CI=false craco build --max_old_space_size=2048",
    "build:dev": "cross-env CI=false REACT_APP_HOST_TYPE=dev craco build --max_old_space_size=2048",
    "stylelint": "stylelint \"src/**/*.scss\" --config stylelint.config.js",
    "i18next:scanner": "i18next-scanner --config i18next-scanner.config.js",
    "lint": "eslint ./src -c .eslintrc.js --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "format": "prettier ./src/**/*.tsx  --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && npm run stylelint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@craco/craco": "^6.1.2",
    "@darwinia/types": "^2.7.2",
    "@fontsource/ibm-plex-sans": "^4.5.2",
    "@metamask/contract-metadata": "^1.28.0",
    "@metamask/jazzicon": "^2.0.0",
    "@polkadot/api": "^7.4.1",
    "@polkadot/extension-dapp": "^0.42.5",
    "@polkadot/react-identicon": "^0.89.2",
    "@polkadot/ui-keyring": "^0.89.3",
    "antd": "^4.16.1",
    "bignumber.js": "^9.0.2",
    "craco-antd": "^1.19.0",
    "date-fns": "^2.22.1",
    "date-fns-tz": "^1.1.6",
    "file-saver": "^2.0.5",
    "graphql-hooks": "^5.2.0",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "i18next-http-backend": "^1.2.6",
    "intro.js": "^5.0.0",
    "intro.js-react": "^0.5.0",
    "is-mobile": "^3.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.2",
    "rxjs": "^7.5.2",
    "store": "^2.0.12",
    "typescript": "^4.1.2",
    "use-deep-compare-effect": "^1.6.1",
    "web-vitals": "^2.1.3",
    "web3": "^1.7.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-angular": "^12.1.4",
    "@commitlint/config-conventional": "^12.0.1",
    "@darwinia/lints": "^1.0.1",
    "@open-wc/webpack-import-meta-loader": "^0.4.7",
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.168",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.2",
    "@types/store": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "ant-theme-generator": "^1.0.1",
    "autoprefixer": "^9.8.6",
    "await-timeout": "^1.1.1",
    "axios": "^0.23.0",
    "bytes32": "^0.0.3",
    "codelyzer": "^6.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.3",
    "i18next-scanner": "^3.0.0",
    "i18next-scanner-typescript": "^1.0.6",
    "node-fetch": "2.6.0",
    "postcss": "^7.0.35",
    "prettier": "^2.2.1",
    "prettier-quick": "0.0.5",
    "pretty-quick": "^3.1.0",
    "sass": "^1.43.4",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-prettier": "^1.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.4",
    "tailwindcss-aspect-ratio": "^3.0.0",
